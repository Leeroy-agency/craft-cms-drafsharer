{
    "name": "leeroy/craft-draft-sharer",
    "description": "Plugin to add a link in the CP to share an entry draft",
    "type": "craft-plugin",
    "version": "1.1.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "draft sharing"
    ],
    "support": {
        "docs": "https://github.com/Leeroy-agency/craft-cms-drafsharer/blob/main/README.md",
        "issues": "https://github.com/Leeroy-agency/craft-cms-drafsharer/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Leeroy agency",
            "homepage": "https://github.com/Leeroy-agency"
        }
    ],
    "require": {
        "php": "^8.0",
        "craftcms/cms": "^4.0.0-alpha"
    },
    "require-dev": {
      "codeception/codeception": "^4.0",
      "vlucas/phpdotenv": "^3.0",
        "craftcms/rector": "dev-main"
    },
    "autoload": {
        "psr-4": {
          "leeroy\\draftsharer\\": "src/"
        }
    },
    "extra": {
        "name": "Draft sharer",
        "handle": "draft-sharer",
        "developer": "Leeroy agency",
        "developerUrl": "https://github.com/Leeroy-agency",
        "documentationUrl": "https://github.com/Leeroy-agency/craft-cms-drafsharer/blob/main/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/Leeroy-agency/craft-cms-drafsharer/main/CHANGELOG.md",
        "class": "leeroy\\draftsharer\\DraftSharer"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
